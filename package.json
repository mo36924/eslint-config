{
  "name": "@mo36924/eslint-config",
  "type": "module",
  "version": "3.0.3",
  "description": "eslint-config",
  "author": "mo36924",
  "license": "MIT",
  "homepage": "https://github.com/mo36924/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mo36924/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/mo36924/eslint-config/issues"
  },
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean lint format build:*",
    "build:rollup": "rollup -c --configPlugin typescript",
    "clean": "del dist",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@antfu/eslint-config": "^2.4.6"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/eslint": "^8.44.9",
    "del-cli": "^5.1.0",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
